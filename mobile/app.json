{
  "expo": {
    "name": "OTTR",
    "slug": "ottr-messaging",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-main.png",
    "scheme": "ottr",
    "userInterfaceStyle": "automatic",
    "description": "One-to-One Exclusive Messaging - Connect with exactly one person at a time for exclusive conversations",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-main.png",
        "backgroundColor": "#A8E6FF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ottr.messaging"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-main.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#A8E6FF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
